				{
					"name": "Authenticate",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"const jsonResponse = pm.response.json;\r",
									"\r",
									"pm.environment.set(\"token\", jsonResponse.token);\r",
									"\r",
									"pm.test(\"Authentication successful\", function () {\r",
